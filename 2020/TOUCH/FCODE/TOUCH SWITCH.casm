004C  1683  	BSF STATUS, RP0
004D  0092  	MOVWF gbl_pr2

	switch (prescaler)
	{
	  #ifdef MX_PWM_PSCA1
		case 1:
004E  3001  	MOVLW 0x01
004F  1283  	BCF STATUS, RP0
0050  064E  	XORWF FC_CAL_PWM_00054_arg_prescaler, W
0051  1903  	BTFSC STATUS,Z
0052  084F  	MOVF FC_CAL_PWM_00054_arg_prescaler+D'1', W
0053  1903  	BTFSC STATUS,Z
0054  2862  	GOTO	label4
0062        label4

			t2con = 0x04;
0062  3004  	MOVLW 0x04
0063  0092  	MOVWF gbl_t2con

			break;
	  #endif
	  #ifdef MX_PWM_PSCA4
		case 4:
0055  3004  	MOVLW 0x04
0056  064E  	XORWF FC_CAL_PWM_00054_arg_prescaler, W
0057  1903  	BTFSC STATUS,Z
0058  084F  	MOVF FC_CAL_PWM_00054_arg_prescaler+D'1', W
0059  1903  	BTFSC STATUS,Z
005A  2865  	GOTO	label5
0065        label5

			t2con = 0x05;
0065  3005  	MOVLW 0x05
0066  0092  	MOVWF gbl_t2con

			break;
	  #endif
	  #ifdef MX_PWM_PSCA16
		case 16:
005B  3010  	MOVLW 0x10
005C  064E  	XORWF FC_CAL_PWM_00054_arg_prescaler, W
005D  1903  	BTFSC STATUS,Z
005E  084F  	MOVF FC_CAL_PWM_00054_arg_prescaler+D'1', W
005F  1903  	BTFSC STATUS,Z
0060  2868  	GOTO	label6
0068        label6

			t2con = 0x06;
0068  3006  	MOVLW 0x06
